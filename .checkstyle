<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
    <local-check-config name="TFA Hub Checks" location=".settings/checkstyle-tfahub.xml" type="project" description="Checkstyle configuration that checks the TFA Hub coding conventions from TFA Hub Java Style that can be found at https://tfahub.github.io/style-guide/java-style-guide.html.">
        <additional-data name="protect-config-file" value="false" />
    </local-check-config>
    <fileset name=".java" enabled="true" check-config-name="TFA Hub Checks" local="true">
        <file-match-pattern match-pattern="\.java" include-pattern="true" />
    </fileset>
</fileset-config>
